{
    "name": "telstra/messagingapi-sdk-php",
    "version": "1.0.4",
    "description": "Telstra Messaging SDK - PHP Library",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "telstradev"
    ],
    "homepage": "https://dev.telstra.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "developersteve",
            "homepage": "https://github.com/developersteve"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Telstra_Messaging\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Telstra_Messaging\\" : "test/" }
    }
}
